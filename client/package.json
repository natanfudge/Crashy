{
  "name": "crashy",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@types/node": "^17.0.21",

    "react": "^17.0.2",
    "@types/react": "^17.0.40",
    "react-dom": "^17.0.2",
    "@types/react-dom": "^17.0.13",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",

    "@mui/material": "^5.10.5",
    "@mui/icons-material": "^5.5.0",
    "@emotion/styled": "^11.8.1",
    "@emotion/react": "^11.8.2",
    "fflate": "^0.7.3",
    "react-infinite-scroll-component": "^6.1.0",

    "@types/jest": "^27.4.1",
    "source-map-explorer": "^2.5.2"
  },
  "scripts": {
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "deploysite": "npm run build && firebase deploy --only hosting",
    "start": "react-scripts start --inspect",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "startprod": "npm run build && serve -s build",
    "profileprod": "npm run build -- --profile && serve -s build"
  },
  "eslintConfig": ".eslintrc.json",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/pako": "^2.0.0",
    "@types/react-window": "^1.8.5",
    "eslint": "^8.23.1",
    "typescript": "^4.8.3",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "eslint-config-fudge-commons": "^1.0.3"
  }
