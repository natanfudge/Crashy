{
  "name": "functions",
  "scripts": {
    "build": "tsc  --p ../functions",
    "serve": "cd ../common && npm run build && npm run build -- --watch | cd ../functions && npm run build -- --watch | firebase emulators:start --inspect-functions",
    "deploy": "firebase deploy --only functions",
    "stop": "kill-port 5000 9229"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^0.26.0",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.18.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^0.3.3",
    "typescript": "^4.5.5"
  },
  "private": true
}
