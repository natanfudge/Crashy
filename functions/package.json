{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc  --p ../functions",
    "serve": "npm run build -- --watch | firebase emulators:start --inspect-functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "stop": "kill-port 5000 9229"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^0.26.0",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.18.0",
    "crashy-common": "1.0.1"
  },
  "devDependencies": {
    "eslint-config-crashy": "1.0.3",
    "firebase-functions-test": "^0.3.3",
    "typescript": "^4.6.2"
  },
  "private": true
}
